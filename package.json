{
  "name": "PostCard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s"
  },
  "dependencies": {
    "@merryjs/photo-viewer": "^2.2.1",
    "apisauce": "^0.14.2",
    "bugsnag-react-native": "^2.9.5",
    "color": "^2.0.1",
    "format-json": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.22.1",
    "prop-types": "^15.6.0",
    "querystringify": "1.0.0",
    "ramda": "^0.25.0",
    "react": "16.3.1",
    "react-native": "0.55.3",
    "react-native-actionsheet": "^2.4.2",
    "react-native-atoz-list": "^1.0.3",
    "react-native-atoz-listview": "github:agiletechvn/react-native-atoz-listview",
    "react-native-button": "^2.3.0",
    "react-native-color": "0.0.10",
    "react-native-color-picker": "^0.4.0",
    "react-native-config": "^0.11.5",
    "react-native-contacts": "^2.2.0",
    "react-native-country-picker-modal": "^0.5.1",
    "react-native-easy-gestures": "^2.0.5",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-flip-component": "^1.0.5",
    "react-native-image-crop-picker": "^0.20.3",
    "react-native-image-gallery": "^2.1.5",
    "react-native-image-placeholder": "^1.0.14",
    "react-native-image-resizer": "^1.0.0",
    "react-native-iphone-x-helper": "^1.0.1",
    "react-native-keyboard-aware-scroll-view": "^0.5.0",
    "react-native-loader-overlay": "^1.0.0",
    "react-native-message-bar": "^1.6.0",
    "react-native-permissions": "^1.1.1",
    "react-native-router-flux": "^4.0.0-beta.27",
    "react-native-shake-event": "^4.0.1",
    "react-native-sortable-listview": "^0.2.8",
    "react-native-text": "0.0.7",
    "react-native-user-avatar": "^1.0.3",
    "react-native-vector-icons": "^4.3.0",
    "react-native-video": "^2.0.0",
    "react-native-video-player": "^0.9.1",
    "react-native-view-shot": "^2.3.0",
    "react-navigation": "^1.0.0-beta.29",
    "react-navigation-redux-helpers": "^1.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0",
    "redux-saga": "^0.16.0",
    "reduxsauce": "0.7.0",
    "seamless-immutable": "^7.1.3",
    "tinycolor2": "^1.4.1",
    "validatorjs": "^3.14.2"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.2.16",
    "@storybook/addons": "^3.2.16",
    "@storybook/channels": "^3.2.16",
    "@storybook/react-native": "^3.2.16",
    "babel-core": "6.26.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "22.4.3",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^3.0.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "husky": "^0.14.3",
    "ignite-fetch-blob": "^1.0.0",
    "ignite-redux-logger": "^0.2.1",
    "ignite-redux-persist": "^1.1.0",
    "ignite-router-flux": "^0.0.12",
    "ignite-standard": "^1.0.0",
    "ignite-vector-icons": "^1.1.0",
    "jest": "22.4.3",
    "mockery": "^2.1.0",
    "react-dom": "16.0.0",
    "react-test-renderer": "^16.3.0-alpha.0",
    "reactotron-react-native": "^1.13.2",
    "reactotron-redux": "^1.13.0",
    "reactotron-redux-saga": "^1.13.0",
    "snazzy": "^7.1.1",
    "standard": "10.0.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/Tests/Setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/Tests/Setup"
    ],
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
